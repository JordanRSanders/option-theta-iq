        <name>Roll_Bottom_or_Top</name>
        <label>Roll Bottom or Top</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Top_Short</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varInRollBottomLongOrTopShort</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Top Short</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Roll_Top_Short_Contracts_Information_Screen</targetReference>
            </connector>
            <label>Top Short</label>
        </rules>
        <rules>
            <name>Bottom_Long</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varInRollBottomLongOrTopShort</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
