<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>64.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <assignments>
        <name>Assign_output_Variables</name>
        <label>Assign output Variables</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <assignmentItems>
            <assignToReference>varOutInputNumberIsDivisibleByTestNumber</assignToReference>
            <operator>Assign</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>varOutNumberOfTimesDivisorGoesIntoTestNumber</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>formTestNumberDividedByDivisor</elementReference>
            </value>
        </assignmentItems>
    </assignments>
    <customProperties>
        <name>ScreenProgressIndicator</name>
        <value>
            <stringValue>{&quot;location&quot;:&quot;top&quot;,&quot;type&quot;:&quot;simple&quot;}</stringValue>
        </value>
    </customProperties>
    <decisions>
        <name>Does_Divisor_Evenly_Divide_Test_Number</name>
        <label>Does Divisor Evenly Divide Test Number</label>
        <locationX>0</locationX>
        <locationY>0</locationY>
        <defaultConnectorLabel>Not Evenly Divisible</defaultConnectorLabel>
        <rules>
            <name>Evenly_Divisible</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>formTestNumberModDivisor</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_output_Variables</targetReference>
            </connector>
            <label>Evenly Divisible</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>formTestNumberDividedByDivisor</name>
        <dataType>Number</dataType>
        <expression>{!varInIntegerToTest} / {!varInDivisorToBeDivisibleBy}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>formTestNumberModDivisor</name>
        <dataType>Number</dataType>
        <expression>MOD({!varInIntegerToTest},{!varInDivisorToBeDivisibleBy})</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>Utility - Is Number Divisible By Divisor {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Utility - Is Number Divisible By Divisor</label>
    <processMetadataValues>
